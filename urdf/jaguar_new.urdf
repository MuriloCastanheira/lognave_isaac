<?xml version="1.0"?>
<robot name="my_robot">

<material name="green">
    <color rgba="0.2 0.8 0.2 1.0"/>
  </material>
<material name="black">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>

  <link name="base_link"/>

    <link name="tractor_link">
        <visual>
        <geometry>
            <mesh filename="package://lognave_isaac/urdf/meshes/tractor.stl" scale="1.0 1.0 1.0"/>
        </geometry>
        <material name="green"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://lognave_isaac/urdf/meshes/tractor.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="63.5"/>
                <inertia  ixx="5.308864583333333" ixy="0.0" ixz="0.0" 
                          iyy="5.993473958333333" iyz="0.0" 
                          izz="5.308864583333333"/>
        </inertial>
    </link>

    <joint name="tractor_joint" type="fixed">
        <parent link="base_link"/>
        <child link="tractor_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>



   <!-- Left Rear Wheel -->

    <link name="left_rear_wheel_link">
        <visual>
        <geometry>
            <mesh filename="package://lognave_isaac/urdf/meshes/left_rear_wheel.stl"
                scale="1.0 1.0 1.0" />
        </geometry>
        <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://lognave_isaac/urdf/meshes/left_rear_wheel.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1.0"/>
                <inertia ixx="0.00225" ixy="0.0" ixz="0.0" 
                         iyy="0.00225" iyz="0.0" 
                         izz="0.00225"/>
        </inertial>
    </link>

    <joint name="left_rear_wheel_joint" type="continuous">
        <origin xyz=" -0.3125 0.1 0.08" rpy="0 0 0" />
        <parent link="tractor_link" />
        <child  link="left_rear_wheel_link" />
        <axis xyz="0 1 0" />

        <calibration falling="0" rising="0" />
        <dynamics damping="0.00" friction="0.0" />
        <safety_controller
            k_position="0"
            k_velocity="0"
            soft_lower="0"
            soft_upper="0" />
    </joint>


   <!-- Right Rear Wheel -->

    <link name="right_rear_wheel_link">
        <visual>
        <geometry>
            <mesh filename="package://lognave_isaac/urdf/meshes/right_rear_wheel.stl"
                scale="1.0 1.0 1.0" />
        </geometry>
        <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://lognave_isaac/urdf/meshes/right_rear_wheel.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1.0"/>
                <inertia ixx="0.00225" ixy="0.0" ixz="0.0" 
                         iyy="0.00225" iyz="0.0" 
                         izz="0.00225"/>
        </inertial>
    </link>

    <joint name="right_rear_wheel_joint" type="continuous">
        <origin xyz=" -0.3125 -0.1 0.08" rpy="0 0 0" />
        <parent link="tractor_link" />
        <child  link="right_rear_wheel_link" />
        <axis xyz="0 1 0" />

        <calibration falling="0" rising="0" />
        <dynamics damping="0.00" friction="0.0" />
        <safety_controller
            k_position="0"
            k_velocity="0"
            soft_lower="0"
            soft_upper="0" />
    </joint>


       <!-- Front fork Yaw -->

    <link name="frot_fork_link">
        <visual>
        <geometry>
            <mesh filename="package://lognave_isaac/urdf/meshes/front_fork.stl"
                scale="1.0 1.0 1.0" />
        </geometry>
        <material name="green"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://lognave_isaac/urdf/meshes/front_fork.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1.0"/>
                <inertia ixx="0.0010000000000000002" ixy="0.0" ixz="0.0" 
                         iyy="0.0010000000000000002" iyz="0.0" 
                         izz="0.0010000000000000002"/>
        </inertial>
    </link>

    <joint name="frot_fork_joint" type="revolute">
        <parent link="tractor_link"/>
        <child link="frot_fork_link"/>
        <origin xyz="0.182 0 0.124" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit effort="10.0" velocity="1.0" lower="-0.65" upper="0.65"/>
    </joint>


   <!-- Front Wheel -->

    <link name="front_wheel_link">
        <visual>
        <geometry>
            <mesh filename="package://lognave_isaac/urdf/meshes/front_wheel.stl"
                scale="1.0 1.0 1.0" />
        </geometry>
        <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://lognave_isaac/urdf/meshes/front_wheel.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1.0"/>
                <inertia ixx="0.0010000000000000002" ixy="0.0" ixz="0.0" 
                         iyy="0.0010000000000000002" iyz="0.0" 
                         izz="0.0010000000000000002"/>
        </inertial>
    </link>

    <joint name="front_wheel_joint" type="continuous">
        <origin xyz=" -0.045 0.0 -0.077" rpy="0 0 0" />
        <parent link="frot_fork_link" />
        <child  link="front_wheel_link" />
        <axis xyz="0 1 0" />

        <calibration falling="0" rising="0" />
        <dynamics damping="0.00" friction="0.0" />
        <safety_controller
            k_position="0"
            k_velocity="0"
            soft_lower="0"
            soft_upper="0" />
    </joint>












</robot>